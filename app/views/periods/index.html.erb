
<%= month_calendar(attribute: :start_date, end_attribute: :end_date, events: @periods) do |date, periods| %>
  <%= date.day %>
  <% periods.each do |period| %>
    <div class="period-day">
      <%= link_to "", period_path(period), class: "calender-button"%>
    </div>
  <% end %>
<% end %>

